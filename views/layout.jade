    link(rel='stylesheet', href='http://fonts.googleapis.com/css?family=Open+Sans:400,700,300' type='text/css')
    link(rel='stylesheet', href='http://fonts.googleapis.com/css?family=Raleway:200' type='text/css' )

    script(src="//cdnjs.cloudflare.com/ajax/libs/jquery/2.0.3/jquery.js")
