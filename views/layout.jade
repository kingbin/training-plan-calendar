    <!-- IE Fix for HTML5 Tags -->
    <!--[if lt IE 9]>
    script(src="//html5shiv.googlecode.com/svn/trunk/html5.js")
    <![endif]-->

  body
    <!-- Navbar -->
    .navbar.navbar-fixed-top
      .navbar-inner
        .container
          a(class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse")
            span.icon-bar
            span.icon-bar
            span.icon-bar
          .nav-collapse.collapse
            ul.nav.pull-right
              li: a(href="#" class="scroller" data-section="#intro") Intro
              li: a(href="#" class="scroller" data-section="#about") About
              li.dropdown
                a(href="#" class="dropdown-toggle" data-toggle="dropdown") Contact Me
                  Dropdown
                  b.caret
                ul.dropdown-menu
                  li: a(href="index.html#") Linked In
                  li: a(href="index.html#") Twitter
                  li: a(href="index.html#") Facebook
      <!-- end Navbar -->

    .main-content
      block content
    footer


    script(type='text/javascript').
      $(function () {
          $(window).scroll(function(){
          // global scroll to top button
              if ($(this).scrollTop() > 300) {
                  $('.scrolltop').fadeIn();
              } else {
                  $('.scrolltop').fadeOut();
              }
          });

          // scroll nav
          $('.scroller').click(function(){
              var section = $($(this).data("section"));
              var top = section.offset().top-82;
              $("html, body").animate({ scrollTop: top }, 700);
              return false;
          });

          // BigVideo Setup
          var BV = new $.BigVideo({useFlashForFirefox:false});
          BV.init();
          if (Modernizr.touch) {
              // fallback image for mobile devices
              BV.show('/assets/img/bg2.jpg');
          } else {
              BV.show('/assets/video/wall.mp4',
              {ambient:true, doLoop:true, altSource:'/assets/video/wall.ogv'});
          }
      });
    block javascript
